    %assign/vec4 v0x5675333c4cf0_0, 0;
    %jmp T_0.7;
T_0.7 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5675333acf20;
T_1 ;
    %delay 5, 0;
