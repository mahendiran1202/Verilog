    %assign/vec4 v0x5675333c4cf0_0, 0;
    %jmp T_0.7;
T_0.2 ;
    %load/vec4 v0x5675333c4cf0_0;
    %assign/vec4 v0x5675333c4cf0_0, 0;
    %jmp T_0.7;
T_0.3 ;
    %load/vec4 v0x5675333c5060_0;
    %load/vec4 v0x5675333c4cf0_0;
    %parti/s 3, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5675333c4cf0_0, 0;
    %jmp T_0.7;
T_0.4 ;
    %load/vec4 v0x5675333c4cf0_0;
    %parti/s 3, 0, 2;
