    .scope S_0x5675333acf20;
T_2 ;
    %vpi_call 2 12 "$monitor", "clock=%d | reset=%d |  mode=%d | shift_right=%b | shift_left=%b | input=%b | output=%b | time=%0t", v0x5675333c5200_0, v0x5675333c5490_0, v0x5675333c5560_0, v0x5675333c5720_0, v0x5675333c5650_0, v0x5675333c52c0_0, v0x5675333c5390_0, $time {0 0 0};
